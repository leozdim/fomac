<div>
  <div class="col s12" style="border-top: 1px solid rgba(160,160,160,0.2); margin-top: 24px;">
    <span class="card-title" style="margin-top: 24px;">Evidencias Musica</span>
    <ul class="collection">

      <li class="collection-item avatar">
        <i class="material-icons circle">folder</i>
        <span class="title">Video de muestra</span>
        <div style="margin-top:16px">
          <%= f.text_field :video, disabled:true%>
          <a class='dropdown-button btn' href='#' data-activates='videomusic'>Validar Video de muestra</a>
          <ul id='videomusic' class='dropdown-content'>
            <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"video",:status=>"Valido", :model=>"music_evidence")%>"data-confirm="¿Estás seguro?">Validar</a></li>
            <%if get_observation_id("video","music_evidence") != ""%>
              <li><a href="<%=edit_revision_path(get_observation_id("video","music_evidence"))%>">Editar observacion</a></li>
            <%else%>
              <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"video",:status=>"Invalido", :model=>"music_evidence")%>">Agregar observaciones</a></li>
            <%end%>
          </ul>
          <%if get_status("video","music_evidence") == "Invalido"%>
            <div class="input-field">
              <textarea class="materialize-textarea" disabled><%=get_observation("video","music_evidence")%></textarea>
            </div>
          <%end%>
        </div>
        <div class="secondary-content">
          <%= get_status("video","music_evidence") %>
        </div>
      </li>






      <li class="collection-item avatar">
        <i class="material-icons circle">folder</i>
        <span class="title">Audio de muestra</span>
        <div style="margin-top:16px">
          <%= f.text_field :audio, disabled:true%>
          <a class='dropdown-button btn' href='#' data-activates='audio'>Validar audio de muestra</a>
          <ul id='audio' class='dropdown-content'>
            <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"audio",:status=>"Valido", :model=>"music_evidence")%>"data-confirm="¿Estás seguro?">Validar</a></li>
            <%if get_observation_id("audio","music_evidence") != ""%>
              <li><a href="<%=edit_revision_path(get_observation_id("audio","music_evidence"))%>">Editar observacion</a></li>
            <%else%>
              <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"audio",:status=>"Invalido", :model=>"music_evidence")%>">Agregar observaciones</a></li>
            <%end%>
          </ul>
          <%if get_status("audio","music_evidence") == "Invalido"%>
            <div class="input-field">
              <textarea class="materialize-textarea" disabled><%=get_observation("audio","music_evidence")%></textarea>
            </div>
          <%end%>
        </div>
        <div class="secondary-content">
          <%= get_status("audio","music_evidence") %>
        </div>
      </li>

      <li class="collection-item avatar">
        <i class="material-icons circle">folder</i>
        <span class="title">Sitio web</span>
        <div style="margin-top:16px">
          <%= f.text_field :web , disabled: true%>
          <a class='dropdown-button btn' href='#' data-activates='webmusic'>Validar Sitio web</a>
          <ul id='webmusic' class='dropdown-content'>
            <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"web",:status=>"Valido", :model=>"music_evidence")%>"data-confirm="¿Estás seguro?">Validar</a></li>
            <%if get_observation_id("web","music_evidence") != ""%>
              <li><a href="<%=edit_revision_path(get_observation_id("web","music_evidence"))%>">Editar observacion</a></li>
            <%else%>
              <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"web",:status=>"Invalido", :model=>"music_evidence")%>">Agregar observaciones</a></li>
            <%end%>
          </ul>
          <%if get_status("web","music_evidence") == "Invalido"%>
            <div class="input-field">
              <textarea class="materialize-textarea" disabled><%=get_observation("web","music_evidence")%></textarea>
            </div>
          <%end%>
        </div>
        <div class="secondary-content">
          <%= get_status("web","music_evidence") %>
        </div>
      </li>



      <li class="collection-item avatar">
        <i class="material-icons circle">folder</i>
        <span class="title">Partituras</span>
        <div style="margin-top:16px">
          <% f.object.score.each do |ob| %>
            <%= link_to 'Descargar Partitura', ob.url , :target=>"_blank", :class => 'waves-effect waves-light btn', :style => 'color:white;'%>
          <%end%>
          <a class='dropdown-button btn' href='#' data-activates='scoremusic'>Validar Partituras</a>
          <ul id='scoremusic' class='dropdown-content'>
            <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"score",:status=>"Valido", :model=>"music_evidence")%>"data-confirm="¿Estás seguro?">Validar</a></li>
            <%if get_observation_id("score","music_evidence") != ""%>
              <li><a href="<%=edit_revision_path(get_observation_id("score","music_evidence"))%>">Editar observacion</a></li>
            <%else%>
              <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"score",:status=>"Invalido", :model=>"music_evidence")%>">Agregar observaciones</a></li>
            <%end%>
          </ul>
          <%if get_status("score","music_evidence") == "Invalido"%>
            <div class="input-field">
              <textarea class="materialize-textarea" disabled><%=get_observation("score","music_evidence")%></textarea>
            </div>
          <%end%>
        </div>
        <div class="secondary-content">
          <%= get_status("score","music_evidence") %>
        </div>
      </li>
      <li class="collection-item avatar">
        <i class="material-icons circle">folder</i>
        <span class="title">Notas de prensa</span>
        <div style="margin-top:16px">
          <% f.object.note.each do |ob| %>
            <%= link_to 'Descargar', ob.url , :target=>"_blank", :class => 'waves-effect waves-light btn', :style => 'color:white;'%>
          <%end%>
          <a class='dropdown-button btn' href='#' data-activates='notemusic'>Validar Notas de prensa</a>
          <ul id='notemusic' class='dropdown-content'>
            <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"note",:status=>"Valido", :model=>"music_evidence")%>"data-confirm="¿Estás seguro?">Validar</a></li>
            <%if get_observation_id("note","music_evidence") != ""%>
              <li><a href="<%=edit_revision_path(get_observation_id("note","music_evidence"))%>">Editar observacion</a></li>
            <%else%>
              <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"note",:status=>"Invalido", :model=>"music_evidence")%>">Agregar observaciones</a></li>
            <%end%>
          </ul>
          <%if get_status("note","music_evidence") == "Invalido"%>
            <div class="input-field">
              <textarea class="materialize-textarea" disabled><%=get_observation("note","music_evidence")%></textarea>
            </div>
          <%end%>
        </div>
        <div class="secondary-content">
          <%= get_status("note","music_evidence") %>
        </div>
      </li>
      <li class="collection-item avatar">
        <i class="material-icons circle">folder</i>
        <span class="title">Invitaciones, constancios y/o reconocimientos</span>
        <div style="margin-top:16px">
          <% f.object.document.each do |ob| %>
            <%= link_to 'Descargar', ob.url , :target=>"_blank", :class => 'waves-effect waves-light btn', :style => 'color:white;'%>
          <%end%>
          <a class='dropdown-button btn' href='#' data-activates='documentmusic'>Validar Invitaciones, constancios y/o reconocimientos</a>
          <ul id='documentmusic' class='dropdown-content'>
            <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"document",:status=>"Valido", :model=>"music_evidence")%>"data-confirm="¿Estás seguro?">Validar</a></li>
            <%if get_observation_id("document","music_evidence") != ""%>
              <li><a href="<%=edit_revision_path(get_observation_id("document","music_evidence"))%>">Editar observacion</a></li>
            <%else%>
              <li><a href="<%=new_revision_path(:project_id => @project.id, :user_id=> @project.user.id,:field=>"document",:status=>"Invalido", :model=>"music_evidence")%>">Agregar observaciones</a></li>
            <%end%>
          </ul>
          <%if get_status("document","music_evidence") == "Invalido"%>
            <div class="input-field">
              <textarea class="materialize-textarea" disabled><%=get_observation("document","music_evidence")%></textarea>
            </div>
          <%end%>
        </div>
        <div class="secondary-content">
          <%= get_status("document","music_evidence") %>
        </div>
      </li>




    </ul>
  </div>
</div>



