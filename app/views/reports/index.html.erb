<p id="notice"><%= notice %></p>

<h1>Reports</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Year</th>
      <th>Period</th>
      <th>Number</th>
      <th>Cuantitative</th>
      <th>Qualitative</th>
      <th>Activity</th>
      <th>Programed date</th>
      <th>Real</th>
      <th>Place</th>
      <th>Number people</th>
      <th>Money received</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= report.project %></td>
        <td><%= report.year %></td>
        <td><%= report.period %></td>
        <td><%= report.number %></td>
        <td><%= report.cuantitative %></td>
        <td><%= report.qualitative %></td>
        <td><%= report.activity %></td>
        <td><%= report.programed_date %></td>
        <td><%= report.real %></td>
        <td><%= report.place %></td>
        <td><%= report.number_people %></td>
        <td><%= report.money_received %></td>
        <td><%= link_to 'Show', report %></td>
        <%#= link_to 'Edit', edit_report_path(report) %>
        <%#= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Report', new_report_path %>
